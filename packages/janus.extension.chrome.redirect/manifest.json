{
    "manifest_version": 2,
    "name": "Janus Name Service Redirect Web3",
    "description": "Janus name service wedirect to web3",
    "version": "1.0.0",
    "icons": {
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>", "http://*/*", "https://*/*"],
        "js": ["redirect.js"],
        "run_at": "document_start"
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs", "<all_urls>", "webNavigation"
    ]
}